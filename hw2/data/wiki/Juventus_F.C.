<li><strong class="selflink">Juventus</strong> (<a href="1995_Supercoppa_Italiana" title="1995 Supercoppa Italiana">1995</a>)</li>
<li><a href="ACF_Fiorentina" title="ACF Fiorentina">Fiorentina</a> (<a href="1996_Supercoppa_Italiana" title="1996 Supercoppa Italiana">1996</a>)</li>
<li><strong class="selflink">Juventus</strong> (<a href="1997_Supercoppa_Italiana" title="1997 Supercoppa Italiana">1997</a>)</li>
<li><a href="../S.S._Lazio" title="S.S. Lazio">Lazio</a> (<a href="1998_Supercoppa_Italiana" title="1998 Supercoppa Italiana">1998</a>)</li>
<li><a href="Parma_F.C." title="Parma F.C.">Parma</a> (<a href="1999_Supercoppa_Italiana" title="1999 Supercoppa Italiana">1999</a>)</li>
<li><a href="../S.S._Lazio" title="S.S. Lazio">Lazio</a> (<a href="2000_Supercoppa_Italiana" title="2000 Supercoppa Italiana">2000</a>)</li>
<li><a href="../A.S._Roma" title="A.S. Roma">Roma</a> (<a href="2001_Supercoppa_Italiana" title="2001 Supercoppa Italiana">2001</a>)</li>
<li><strong class="selflink">Juventus</strong> (<a href="2002_Supercoppa_Italiana" title="2002 Supercoppa Italiana">2002</a>; <a href="2003_Supercoppa_Italiana" title="2003 Supercoppa Italiana">2003</a>)</li>
<li><a href="../A.C._Milan" title="A.C. Milan">Milan</a> (<a href="2004_Supercoppa_Italiana" title="2004 Supercoppa Italiana">2004</a>)</li>
<li><a href="Inter_Milan" title="Inter Milan">Internazionale</a> (<a href="2005_Supercoppa_Italiana" title="2005 Supercoppa Italiana">2005</a>; <a href="2006_Supercoppa_Italiana" title="2006 Supercoppa Italiana">2006</a>)</li>
<li><a href="../A.S._Roma" title="A.S. Roma">Roma</a> (<a href="2007_Supercoppa_Italiana" title="2007 Supercoppa Italiana">2007</a>)</li>
<li><a href="Inter_Milan" title="Inter Milan">Internazionale</a> (<a href="2008_Supercoppa_Italiana" title="2008 Supercoppa Italiana">2008</a>)</li>
<li><a href="../S.S._Lazio" title="S.S. Lazio">Lazio</a> (<a href="2009_Supercoppa_Italiana" title="2009 Supercoppa Italiana">2009</a>)</li>
<li><a href="Inter_Milan" title="Inter Milan">Internazionale</a> (<a href="2010_Supercoppa_Italiana" title="2010 Supercoppa Italiana">2010</a>)</li>
<li><a href="../A.C._Milan" title="A.C. Milan">Milan</a> (<a href="2011_Supercoppa_Italiana" title="2011 Supercoppa Italiana">2011</a>)</li>
<li><strong class="selflink">Juventus</strong> (<a href="2012_Supercoppa_Italiana" title="2012 Supercoppa Italiana">2012</a>)</li>
</ul>
</div>
</td>
</tr>
