<area href="Nix_(moon)" shape="circle" coords="78,33,1" alt="Nix" title="Nix" />
<area href="Hydra_(moon)" shape="circle" coords="95,67,1" alt="Hydra" title="Hydra" />
<area href="S/2011_P_1" shape="circle" coords="97,36,1" alt="S/2011 (134340) 1" title="S/2011 (134340) 1" />
<area href="Pluto" shape="circle" coords="83,51,15" alt="Pluto" title="Pluto" />
<area href="Makemake_(dwarf_planet)" shape="circle" coords="143,49,11" alt="Makemake" title="Makemake" />
