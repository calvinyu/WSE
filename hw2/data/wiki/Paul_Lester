			<h1 id="firstHeading" class="firstHeading" lang="en"><span dir="auto">Paul Lester</span></h1>
			<!-- /firstHeading -->
			<!-- bodyContent -->
			<div id="bodyContent">
								<!-- tagline -->
				<div id="siteSub">From Wikipedia, the free encyclopedia</div>
