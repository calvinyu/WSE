						<!-- firstHeading -->
			<h1 id="firstHeading" class="firstHeading" lang="en"><span dir="auto">Goo Goo Dolls</span></h1>
			<!-- /firstHeading -->
			<!-- bodyContent -->
			<div id="bodyContent">
								<!-- tagline -->
				<div id="siteSub">From Wikipedia, the free encyclopedia</div>
				<!-- /tagline -->
								<!-- subtitle -->
				<div id="contentSub">  (Redirected from <a href="../../w/index-title=The_Goo_Goo_Dolls&redirect=no.php.html" title="The Goo Goo Dolls">The Goo Goo Dolls</a>)</div>
				<!-- /subtitle -->
																<!-- jumpto -->
