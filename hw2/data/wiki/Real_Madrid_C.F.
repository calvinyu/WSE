<li><span class="vevent"><span class="agent attendee vcard"><span class="fn org summary"><a href="Ram%C3%B3n_Encinas" title="Ramón Encinas">Encinas</a></span></span> (<span class="dtstart">1943</span>–45)</span></li>
<li><span class="vevent"><span class="agent attendee vcard"><span class="fn org summary"><a href="Jacinto_Quincoces" title="Jacinto Quincoces">Quincoces</a></span></span> (<span class="dtstart">1945</span>–46)</span></li>
<li><span class="vevent"><span class="agent attendee vcard"><span class="fn org summary"><a href="Baltasar_Alb%C3%A9niz" title="Baltasar Albéniz">Albéniz</a></span></span> (<span class="dtstart">1946</span>–47)</span></li>
<li><span class="vevent"><span class="agent attendee vcard"><span class="fn org summary"><a href="Jacinto_Quincoces" title="Jacinto Quincoces">Quincoces</a></span></span> (<span class="dtstart">1947</span>–48)</span></li>
<li><span class="vevent"><span class="agent attendee vcard"><span class="fn org summary"><a href="Michael_Keeping" title="Michael Keeping">Keeping</a></span></span> (<span class="dtstart">1948</span>–50)</span></li>
<li><span class="vevent"><span class="agent attendee vcard"><span class="fn org summary"><a href="Baltasar_Alb%C3%A9niz" title="Baltasar Albéniz">Albéniz</a></span></span> (<span class="dtstart">1950</span>–51)</span></li>
<li><span class="vevent"><span class="agent attendee vcard"><span class="fn org summary"><a href="H%C3%A9ctor_Scarone" title="Héctor Scarone">Scarone</a></span></span> (<span class="dtstart">1951</span>–52)</span></li>
<li><span class="vevent"><span class="agent attendee vcard"><span class="fn org summary"><a href="Juan_Antonio_Ipi%C3%B1a" title="Juan Antonio Ipiña">Ipiña</a></span></span> (<span class="dtstart">1952</span>–53)</span></li>
<li><span class="vevent"><span class="agent attendee vcard"><span class="fn org summary"><a href="Enrique_Fern%C3%A1ndez_Viola" title="Enrique Fernández Viola">Fernández</a></span></span> (<span class="dtstart">1953</span>–54)</span></li>
<li><span class="vevent"><span class="agent attendee vcard"><span class="fn org summary"><a href="Jos%C3%A9_Villalonga_Llorente" title="José Villalonga Llorente">Villalonga</a></span></span> (<span class="dtstart">1954</span>–57)</span></li>
<li><span class="vevent"><span class="agent attendee vcard"><span class="fn org summary"><a href="Luis_Carniglia" title="Luis Carniglia">Carniglia</a></span></span> (<span class="dtstart">1957</span>–59)</span></li>
<li><span class="vevent"><span class="agent attendee vcard"><span class="fn org summary"><a href="Miguel_Mu%C3%B1oz" title="Miguel Muñoz">Muñoz</a></span></span> (<span class="dtstart">1959</span>)</span></li>
<li><span class="vevent"><span class="agent attendee vcard"><span class="fn org summary"><a href="Luis_Carniglia" title="Luis Carniglia">Carniglia</a></span></span> (<span class="dtstart">1959</span>)</span></li>
<li><span class="vevent"><span class="agent attendee vcard"><span class="fn org summary"><a href="Manuel_Fleitas_Solich" title="Manuel Fleitas Solich">Fleitas</a></span></span> (<span class="dtstart">1959</span>–60)</span></li>
<li><span class="vevent"><span class="agent attendee vcard"><span class="fn org summary"><a href="Miguel_Mu%C3%B1oz" title="Miguel Muñoz">Muñoz</a></span></span> (<span class="dtstart">1960</span>–74)</span></li>
<li><span class="vevent"><span class="agent attendee vcard"><span class="fn org summary"><a href="Luis_Molowny" title="Luis Molowny">Molowny</a></span></span> (<span class="dtstart">1974</span>)</span></li>
