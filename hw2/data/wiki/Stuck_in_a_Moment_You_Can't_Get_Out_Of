						<!-- firstHeading -->
			<h1 id="firstHeading" class="firstHeading" lang="en"><span dir="auto">Stuck in a Moment You Can't Get Out Of</span></h1>
			<!-- /firstHeading -->
			<!-- bodyContent -->
