						<!-- firstHeading -->
			<h1 id="firstHeading" class="firstHeading" lang="en"><span dir="auto">Snap music</span></h1>
			<!-- /firstHeading -->
			<!-- bodyContent -->
			<div id="bodyContent">
								<!-- tagline -->
				<div id="siteSub">From Wikipedia, the free encyclopedia</div>
				<!-- /tagline -->
								<!-- subtitle -->
				<div id="contentSub"></div>
				<!-- /subtitle -->
																<!-- jumpto -->
				<div id="jump-to-nav" class="mw-jump">
					Jump to:					<a href="#mw-head">navigation</a>, 					<a href="#p-search">search</a>
				</div>
				<!-- /jumpto -->
								<!-- bodycontent -->
				<div id="mw-content-text" lang="en" dir="ltr" class="mw-content-ltr"><table class="infobox hlist" cellspacing="3" style="border-spacing: 3px; width:22em;">
<tr>
<th colspan="2" class="" style="text-align:center; font-size:125%; font-weight:bold; background-color: darkblue; color:white">Snap music</th>
</tr>
<tr class="">
<th scope="row" style="text-align:left;">Stylistic origins</th>
<td class="" style="">Genres of music:<br />
