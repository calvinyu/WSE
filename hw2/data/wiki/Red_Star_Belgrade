<li><span class="vevent"><span class="agent attendee vcard"><span class="fn org summary"><a href="Ivan_Toplak" title="Ivan Toplak">Toplak</a></span></span> (<span class="dtstart">1964</span>–66)</span></li>
<li><span class="vevent"><span class="agent attendee vcard"><span class="fn org summary"><a href="Miljan_Miljani%C4%87" title="Miljan Miljanić">Miljanić</a></span></span> (<span class="dtstart">1966</span>–74)</span></li>
<li><span class="vevent"><span class="agent attendee vcard"><span class="fn org summary"><a href="Miljenko_Mihi%C4%87" title="Miljenko Mihić">Mihić</a></span></span> (<span class="dtstart">1974</span>–75)</span></li>
<li><span class="vevent"><span class="agent attendee vcard"><span class="fn org summary"><a href="Milovan_%C4%86iri%C4%87" title="Milovan Ćirić">Ćirić</a></span></span> (<span class="dtstart">1975</span>–76)</span></li>
<li><span class="vevent"><span class="agent attendee vcard"><span class="fn org summary"><a href="Gojko_Zec" title="Gojko Zec">Zec</a></span></span> (<span class="dtstart">1976</span>–78)</span></li>
<li><span class="vevent"><span class="agent attendee vcard"><span class="fn org summary"><a href="Branko_Stankovi%C4%87" title="Branko Stanković">Stanković</a></span></span> (<span class="dtstart">1978</span>–81)</span></li>
<li><span class="vevent"><span class="agent attendee vcard"><span class="fn org summary"><a href="Stevan_Ostoji%C4%87" title="Stevan Ostojić">Ostojić</a></span></span> (<span class="dtstart">1981</span>–83)</span></li>
