<li><span class="vevent"><span class="agent attendee vcard"><span class="fn org summary"><a href="Ljupko_Petrovi%C4%87" title="Ljupko Petrović">Lj. Petrović</a></span></span> (<span class="dtstart">1990</span>–91)</span></li>
<li><span class="vevent"><span class="agent attendee vcard"><span class="fn org summary"><a href="Vladica_Popovi%C4%87" title="Vladica Popović">Popović</a></span></span> (<span class="dtstart">1991</span>–92)</span></li>
<li><span class="vevent"><span class="agent attendee vcard"><span class="fn org summary"><a href="Milan_%C5%BDivadinovi%C4%87" title="Milan Živadinović">Živadinović</a></span></span> (<span class="dtstart">1992</span>–94)</span></li>
<li><span class="vevent"><span class="agent attendee vcard"><span class="fn org summary"><a href="Ljupko_Petrovi%C4%87" title="Ljupko Petrović">Lj. Petrović</a></span></span> (<span class="dtstart">1994</span>–96)</span></li>
