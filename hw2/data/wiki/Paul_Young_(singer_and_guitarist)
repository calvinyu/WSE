						<!-- firstHeading -->
			<h1 id="firstHeading" class="firstHeading" lang="en"><span dir="auto">Paul Young</span></h1>
			<!-- /firstHeading -->
			<!-- bodyContent -->
			<div id="bodyContent">
								<!-- tagline -->
				<div id="siteSub">From Wikipedia, the free encyclopedia</div>
				<!-- /tagline -->
								<!-- subtitle -->
				<div id="contentSub">  (Redirected from <a href="../../w/index-title=Paul_Young_(singer_and_guitarist)&redirect=no.php.html" title="Paul Young (singer and guitarist)">Paul Young (singer and guitarist)</a>)</div>
				<!-- /subtitle -->
