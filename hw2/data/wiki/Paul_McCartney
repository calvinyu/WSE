<li>"<a href="Mrs_Vandebilt" title="Mrs Vandebilt">Mrs Vandebilt</a>" / "<a href="Bluebird_(song)" title="Bluebird (song)">Bluebird</a> (1973)"</li>
<li>"<a href="Band_on_the_Run_(song)" title="Band on the Run (song)">Band on the Run</a>" / "<a href="Zoo_Gang_(song)" title="Zoo Gang (song)">Zoo Gang</a>" (UK) (1974)</li>
<li>"<a href="Band_on_the_Run_(song)" title="Band on the Run (song)">Band on the Run</a>" / "<a href="Nineteen_Hundred_and_Eighty-Five" title="Nineteen Hundred and Eighty-Five">Nineteen Hundred and Eighty-Five</a>" (US) (1974)</li>
<li>"<a href="Junior's_Farm" title="Junior's Farm">Junior's Farm</a>" / "<a href="Sally_G" title="Sally G">Sally G</a>" (1974)</li>
<li>"Walking in the Park with Eloise" / "Bridge Over the River Suite" (1974)</li>
<li>"<a href="Listen_to_What_the_Man_Said" title="Listen to What the Man Said">Listen to What the Man Said</a>" / "<a href="Love_in_Song" title="Love in Song">Love in Song</a>" (1975)</li>
<li>"<a href="Letting_Go_(Wings_song)" title="Letting Go (Wings song)">Letting Go</a>" / "<a href="You_Gave_Me_the_Answer" title="You Gave Me the Answer">You Gave Me the Answer</a>" (1975)</li>
<li>"<a href="Venus_and_Mars/Rock_Show" title="Venus and Mars/Rock Show">Venus and Mars/Rock Show</a>" / "<a href="Magneto_and_Titanium_Man" title="Magneto and Titanium Man">Magneto and Titanium Man</a>" (1975)</li>
<li>"<a href="Silly_Love_Songs" title="Silly Love Songs">Silly Love Songs</a>" / "<a href="Cook_of_the_House" title="Cook of the House">Cook of the House</a>" (1976)</li>
<li>"<a href="Let_'Em_In" title="Let 'Em In">Let 'Em In</a>" / "<a href="Beware_My_Love" title="Beware My Love">Beware My Love</a>" (1976)</li>
