						<!-- firstHeading -->
			<h1 id="firstHeading" class="firstHeading" lang="en"><span dir="auto">BBC Online</span></h1>
			<!-- /firstHeading -->
			<!-- bodyContent -->
			<div id="bodyContent">
								<!-- tagline -->
				<div id="siteSub">From Wikipedia, the free encyclopedia</div>
				<!-- /tagline -->
								<!-- subtitle -->
				<div id="contentSub"></div>
				<!-- /subtitle -->
																<!-- jumpto -->
				<div id="jump-to-nav" class="mw-jump">
					Jump to:					<a href="#mw-head">navigation</a>, 					<a href="#p-search">search</a>
				</div>
				<!-- /jumpto -->
								<!-- bodycontent -->
				<div id="mw-content-text" lang="en" dir="ltr" class="mw-content-ltr"><table class="infobox" cellspacing="3" style="border-spacing: 3px; width:22em;">
<caption class="" style="">BBC Online</caption>
<tr class="">
<td colspan="2" class="" style="text-align:center;"><a href="http://en.wikipedia.org/wiki/File:BBC_Homepage_November_2011.png" class="image"><img alt="BBC Homepage November 2011.png" src="http://upload.wikimedia.org/wikipedia/en/thumb/4/49/BBC_Homepage_November_2011.png/250px-BBC_Homepage_November_2011.png" width="250" height="250" srcset="//upload.wikimedia.org/wikipedia/en/thumb/4/49/BBC_Homepage_November_2011.png/375px-BBC_Homepage_November_2011.png 1.5x, //upload.wikimedia.org/wikipedia/en/thumb/4/49/BBC_Homepage_November_2011.png/500px-BBC_Homepage_November_2011.png 2x" /></a></td>
</tr>
<tr class="">
