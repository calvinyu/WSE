<ul>
<li><span class="vevent"><span class="agent attendee vcard"><span class="fn org summary">Selection committee</span></span> (<span class="dtstart">1892</span>–1930)</span></li>
<li><span class="vevent"><span class="agent attendee vcard"><span class="fn org summary"><a href="Andy_Cunningham_(footballer)" title="Andy Cunningham (footballer)">Cunningham</a></span></span> (<span class="dtstart">1930</span>–35)</span></li>
<li><span class="vevent"><span class="agent attendee vcard"><span class="fn org summary"><a href="Tom_Mather" title="Tom Mather">Mather</a></span></span> (<span class="dtstart">1935</span>–39)</span></li>
<li><span class="vevent"><span class="agent attendee vcard"><span class="fn org summary"><a href="Stan_Seymour" title="Stan Seymour">Seymour</a></span></span> (<span class="dtstart">1939</span>–47)</span></li>
<li><span class="vevent"><span class="agent attendee vcard"><span class="fn org summary"><a href="George_Martin_(footballer)" title="George Martin (footballer)">Martin</a></span></span> (<span class="dtstart">1947</span>–50)</span></li>
