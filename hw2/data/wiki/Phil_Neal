<li><span class="vevent"><span class="agent attendee vcard"><span class="fn org summary"><a href="Joe_Royle" title="Joe Royle">Royle</a></span></span> (<span class="dtstart">1998</span>–2001)</span></li>
<li><span class="vevent"><span class="agent attendee vcard"><span class="fn org summary"><a href="Kevin_Keegan" title="Kevin Keegan">Keegan</a></span></span> (<span class="dtstart">2001</span>–05)</span></li>
<li><span class="vevent"><span class="agent attendee vcard"><span class="fn org summary"><a href="Stuart_Pearce" title="Stuart Pearce">Pearce</a></span></span> (<span class="dtstart">2005</span>–07)</span></li>
<li><span class="vevent"><span class="agent attendee vcard"><span class="fn org summary"><a href="Sven-G%C3%B6ran_Eriksson" title="Sven-Göran Eriksson">Eriksson</a></span></span> (<span class="dtstart">2007</span>–08)</span></li>
<li><span class="vevent"><span class="agent attendee vcard"><span class="fn org summary"><a href="Mark_Hughes" title="Mark Hughes">Hughes</a></span></span> (<span class="dtstart">2008</span>–09)</span></li>
