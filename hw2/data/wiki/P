ay/mwgadget.GuidedTour\"\x3eto GitHub\x3c/a\x3e, for instance.","wgFlaggedRevsParams":{"tags":{"status":{"levels":1,"quality":2,"pristine":3}}},"wgStableRevisionId":null,"wgCategoryTreePageCategoryOptions":"{\"mode\":0,\"hideprefix\":20,\"showcount\":true,\"namespaces\":false}","Geo":{"city":"","country":""},"wgNoticeProject":"wikipedia","aftv5Article":{"id":23342,"title":"P","namespace":0,"categories":["All articles with unsourced statements","Articles with unsourced statements from March 2012","ISO basic Latin letters","Wikipedia indefinitely move-protected pages"],"permissionLevel":"aft-reader"}});
