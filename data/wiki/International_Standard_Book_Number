        <span class="br0">}</span> <span class="kw1">else</span> <span class="br0">{</span><span class="co1">//running the loop</span>
            <span class="re0">$a</span> <span class="sy0">+=</span> <span class="kw3">intval</span><span class="br0">(</span><span class="re0">$ISBN10</span><span class="br0">[</span><span class="re0">$i</span><span class="br0">]</span><span class="br0">)</span> <span class="sy0">*</span> <span class="kw3">intval</span><span class="br0">(</span><span class="nu0">10</span><span class="sy0">-</span><span class="re0">$i</span><span class="br0">)</span><span class="sy0">;</span>
        <span class="br0">}</span>
    <span class="br0">}</span>
    <span class="kw1">return</span> <span class="br0">(</span><span class="re0">$a</span> <span class="sy0">%</span> <span class="nu0">11</span> <span class="sy0">==</span> <span class="nu0">0</span><span class="br0">)</span><span class="sy0">;</span>
<span class="br0">}</span>
</pre></div>
