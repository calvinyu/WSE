<!-- LANGUAGES -->
<div class="portal" role="navigation" id='p-lang'>
	<h3>Languages</h3>
	<div class="body">
		<ul>
			<li class="interwiki-af"><a href="http://af.wikipedia.org/wiki/Natuurlike_getal" title="Natuurlike getal" lang="af" hreflang="af">Afrikaans</a></li>
